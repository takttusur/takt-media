{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Aspire": {
    "StackExchange": {
      "Redis": {
        "ConfigurationOptions": {
          "ConnectTimeout": 3000,
          "ConnectRetry": 2
        },
        "HealthChecks": false,
        "Tracing": false,
        "ConnectionString": ""
      }
    }
  },
  "Quartz": {
    "": ""
  },
  "QuartzJobs": {
    "": ""
  },
  "TextRestrictions": {
    "ShortArticleMaxSymbolsCount": 0,
    "ShortArticleMaxParagraphs": 0
  },
  "JobsTimetableConfiguration": {
    "NewsReplicationJob" : "* */10 * * * ?"
  },
  "NewsReplicationJobConfiguration": {
    "IsEnabled": true,
    "SourceIdentifier": "",
    "MaxSymbolsCount": 500,
    "MaxParagraphCount": 500,
    "MaxArticlesCount": 100,
    "CommitBuffer": 10
  }
}